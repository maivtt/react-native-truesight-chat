{
  "name": "react-native-truesight-chat",
  "version": "0.1.3-rc.6",
  "description": "react native truesight app chat",
  "main": "src/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "!dist",
    "assets",
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-truesight-chat.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "extract": "i18next-extractor -i src/ -o src/i18n/ -p src/i18n/ extract",
    "merge": "i18next-extractor -i src/ -o src/i18n/ -p src/i18n/ merge"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/maivtt/react-native-truesight-chat",
  "author": "maivtt14 <vungoc2901@gmail.com> (https://github.com/maivtt)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maivtt/react-native-truesight-chat/issues"
  },
  "homepage": "https://github.com/maivtt/react-native-truesight-chat#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-native": "^0.67.8",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.18.0",
    "react-native-css-module": "^1.0.0",
    "react-native-svg-types": "^1.0.0",
    "react3l-i18next-extractor": "^0.4.0",
    "release-it": "^14.2.2",
    "rxjs": "^7.8.0",
    "ts-nameof.macro": "^4.2.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "^2.6.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "configFile": "./babel.config.js",
          "copyFlow": true
        }
      ],
      [
        "module",
        {
          "configFile": "./babel.config.js",
          "copyFlow": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@react-native-community/cameraroll": "^4.1.2",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "fbjs": "^3.0.4",
    "moment": "^2.29.3",
    "react-native-atomic-styles": "^0.0.2",
    "react-native-blob-util": "^0.16.0",
    "react-native-document-picker": "^8.1.1",
    "react-native-file-viewer": "^2.1.5",
    "react-native-image-viewing": "^0.2.2",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "^5.4.24",
    "react-native-permissions": "^3.3.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-redux-theming": "^0.1.8",
    "react-native-spinkit": "^1.5.1",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^3.1.1",
    "react3l": "^7.0.0-rc3",
    "react3l-localization": "^6.0.0",
    "reflect-metadata": "^0.1.13"
  }
}
