{
  "name": "react-native-truesight-chat-example",
  "description": "Example app for react-native-truesight-chat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "fresh-start": "yarn start --reset-cache",
    "extract": "i18next-extractor -i a/ -o a/i18n/ -p a/i18n/ extract",
    "merge": "i18next-extractor -i a/ -o a/i18n/ -p a/i18n/ merge",
    "install-all": "yarn install && yarn pod",
    "pod": "cd ios && pod install && cd ..",
    "clean": "yarn clean-ios && yarn clean-android && yarn clean-node && watchman watch-del-all && rm -rf /tmp/metro-*",
    "clean-android": "rm -rf android/.gradle android/.idea android/build android/app/build",
    "clean-ios": "cd ios && pod deintegrate && cd .. && pod cache clean --all && rm -rf ios/build ios/Podfile.lock",
    "clean-node": "rm -rf node_modules yarn.lock",
    "clean-npm": "npm cache clean --force"
  },
  "dependencies": {
    "@microsoft/signalr": "^6.0.6",
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-native-community/cameraroll": "^4.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-native": "^0.67.8",
    "axios": "~0.21",
    "build-url": "^6.0.1",
    "i18next": "^21.8.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "react": "16.13.1",
    "react-i18next": "^11.17.0",
    "react-native": "0.63.4",
    "react-native-atomic-styles": "^0.0.2",
    "react-native-blob-util": "^0.16.0",
    "react-native-device-info": "^9.0.2",
    "react-native-document-picker": "^8.1.0",
    "react-native-file-viewer": "^2.1.5",
    "react-native-flipper": "^0.127.0",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-image-helper": "^0.0.3",
    "react-native-image-viewing": "^0.2.2",
    "react-native-pager-view": "^5.4.24",
    "react-native-permissions": "^3.3.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "2.8.0",
    "react-native-redux-theming": "^0.1.6",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.13.1",
    "react-native-spinkit": "^1.5.1",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-toasty": "^2.0.0",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.2",
    "react3l": "^7.0.0-rc3",
    "react3l-advanced-filters": "^6.0.0",
    "react3l-axios-observable": "^6.0.0",
    "react3l-common": "^6.0.0",
    "react3l-decorators": "^6.0.0",
    "react3l-localization": "^6.0.0",
    "reactn": "^2.2.7",
    "redux": "^4.2.0",
    "redux-flipper": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rn-redux-middleware-flipper": "^0.1.0",
    "rxjs": "^7.8.0",
    "ts-nameof.macro": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/runtime": "^7.12.5",
    "@types/lodash": "^4.14.182",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "metro-config": "^0.71.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "metro-react-native-babel-transformer": "^0.71.0",
    "postcss": "^8.4.14",
    "react-native-postcss-transformer": "^1.2.4",
    "react-native-sass-transformer": "^2.0.0",
    "sass": "^1.52.2"
  }
}
